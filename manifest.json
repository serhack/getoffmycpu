{
  "manifest_version": 2,

  "name": "Get Off My CPU",
  "description": "Block Hidden Miner",
  "version": "0.1.0",

    "background": {
		"scripts": ["background.js"],
		"persistent": true
	},
    
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
      "tabs",
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
  ]
}
